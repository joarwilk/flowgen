{
  "name": "flowgen",
  "description": "Generate flowtype definition files from TypeScript",
  "typings": "index.d.ts",
  "version": "1.14.1",
  "bin": {
    "flowgen": "./lib/cli/index.js"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts",
      "!**/*.d.ts"
    ]
  },
  "dependencies": {
    "@babel/code-frame": "^7.10.4",
    "@babel/highlight": "^7.10.4",
    "commander": "^6.1.0",
    "lodash": "^4.17.20",
    "prettier": "^2.1.1",
    "shelljs": "^0.8.4",
    "typescript": "^4.0.2",
    "typescript-compiler": "^1.4.1-2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/babel__code-frame": "^7.0.1",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.10.0",
    "@types/prettier": "^2.1.0",
    "@types/react": "^16.9.49",
    "@types/shelljs": "^0.8.7",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "chalk": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "flow-bin": "^0.141.0",
    "jest": "^26.4.2"
  },
  "files": [
    "lib",
    "index.d.ts",
    "index.js.flow"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "babel ./src --extensions '.ts,.tsx,.js' --out-dir lib --delete-dir-on-start --ignore 'src/**/*.spec.ts' --ignore 'src/__tests__/'",
    "compile:watch": "npm run compile -- -w",
    "test": "jest src/**/*.spec.ts",
    "test:watch": "jest src/**/*.spec.ts --watch",
    "lint": "eslint '**/*.{js,ts,tsx}' --quiet"
  }
}
